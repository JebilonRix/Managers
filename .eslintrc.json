{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["eslint:recommended", "plugin:react/recommended"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": ["react"],
    "rules": {
        "brace-style": ["warn", "allman", { "allowSingleLine": false }],
        "camelcase": 1,
        "no-await-in-loop": 1,
        "no-duplicate-case": 1,
        "no-duplicate-imports": 1,
        "no-multiple-empty-lines": ["warn", { "max": 1, "maxEOF": 0 }],
        "no-trailing-spaces": "warn",
        "no-undef": 0,
        "no-unused-vars": 0,
        "quotes": ["warn", "double"],
        "react/prop-types": 0
    }
}
